#include<stdio.h>

int main()
{
    int array[100],s,i,n,position=0;
    
    printf("enter the number of elements in array \n");
    scanf("%d",&n);
    
     printf("enter elements\n");
     
     for(i=0; i<n; i++){
         scanf("%d",&array[i]);
         }
         
     printf("enter the number to be search\n");
     scanf("%d",&s);
     
     for(i=0; i<n; i++){
         if(array[i]==s)
         position=i+1;
         
     }
         if(position!=0){
         printf("element is present at position %d",i+1);
         }
         else{
             printf("%d is not present",s);
         }
         }
